{
  "$schema": "https://raw.githubusercontent.com/Ylianst/MeshCentral/master/meshcentral-config-schema.json",
  "settings": {
    "mongoDb": null,
    "WANonly": true,
    "sessionKey": "SESSION_KEY_PLACEHOLDER",
    "port": 10000,
    "aliasPort": 443,
    "redirPort": 0,
    "agentPort": 0,
    "agentAliasPort": 443,
    "cert": "DOMAIN_PLACEHOLDER",
    "tlsOffload": true,
    "trustedProxy": "0.0.0.0/0",
    "allowLoginToken": true,
    "allowFraming": true,
    "webRTC": false,
    "compression": true,
    "wsCompression": true,
    "agentCoreDump": false,
    "exactPorts": true,
    "newAccounts": true,
    "cookieIpCheck": false,
    "cookieEncoding": "hex",
    "AmtScanner": false,
    "AmtManager": false,
    "LANonly": false,
    "noUsers": false
  },
  "domains": {
    "": {
      "title": "NET8 Remote Management",
      "title2": "41 Windows PC Manager",
      "newAccounts": true,
      "userNameIsEmail": false,
      "minify": true,
      "novnc": true,
      "mstsc": true,
      "ssh": true,
      "agentInviteCodes": true,
      "deviceMeshRouterLinks": {
        "rdp": true,
        "ssh": true,
        "scp": true
      },
      "footer": "NET8 Remote Management System"
    }
  }
